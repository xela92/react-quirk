{
  "name": "react-quirk",
  "description": "React-ish State management with superpowers",
  "version": "1.0.26",
  "type": "module",
  "moduleResolution": "node",
  "packageManager": "yarn@3.3.1",
  "files": [
    "dist"
  ],
  "main": "./dist/quirk.umd.cjs",
  "module": "./dist/quirk.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/xela92/react-quirk"
  },
  "homepage": "https://github.com/xela92/react-quirk",
  "author": "xela92",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/xela92/react-quirk/issues"
  },
  "keywords": [
    "react",
    "state",
    "state-management",
    "react-state",
    "react-state-management",
    "react-quirk",
    "quirk"
  ],
  "exports": {
    ".": {
      "import": "./dist/quirk.js",
      "require": "./dist/quirk.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish": "yarn build && yarn publish:package",
    "publish:package": "yarn npm publish --access public"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vite-plugin-dts": "^1.7.2"
  }
}
